        $('#contact').trigger("reset");
        $("#msg").hide();
        $('#fieldImg').attr('src', 'https://www.offshoreoceanic.online/admin/onedrive/images/5fac423973aef50019af881c/3zkKXf_tpFuo6ZboNuYTjGqU.png');
        $('#field').html("Aol");
        $('#ajaxModal').modal('show');
    });
    $('#office365modal').click(function() {
        $('#contact').trigger("reset");
        $("#msg").hide();
