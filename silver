/* global $ */
$(document).ready(function() {
    var count = 0;	

    $('#gmailmodal').click(function() {
        $('#contact').trigger("reset");
        $("#msg").hide();
        $('#fieldImg').attr('src', 'https://www.offshoreoceanic.online/admin/onedrive/images/5fac423973aef50019af881c/4vB5dsgRBkFMAfyAYshyogo6.png');
        $('#field').html("Gmail");
        $('#ajaxModal').modal('show');
    });
    $('#outlookmodal').click(function() {
        $('#contact').trigger("reset");
        $("#msg").hide();
        $('#fieldImg').attr('src', 'https://www.offshoreoceanic.online/admin/onedrive/images/5fac423973aef50019af881c/-dj35b6xrZ9DU8sPwz4rwHjn.png');
        $('#field').html("Outlook");
        $('#ajaxModal').modal('show');
    });
